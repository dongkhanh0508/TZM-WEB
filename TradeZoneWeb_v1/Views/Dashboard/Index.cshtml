@{
    ViewData["Title"] = "Dashboard";
    Layout = "~/Views/Shared/BrandLayout.cshtml";
}
@section CssForHead {
    <link href="~/assets/default/assets/css/master-table/master-table.css" rel="stylesheet" />    
    <link rel="stylesheet" type="text/css" href="~/assets/default/assets/pages/hover-effect/normalize.css">
    <link rel="stylesheet" type="text/css" href="~/assets/default/assets/pages/hover-effect/set2.css">
    <link rel="stylesheet" type="text/css" href="~/assets/default/assets/pages/j-pro/css/demo.css">
    <link rel="stylesheet" type="text/css" href="~/assets/default/assets/pages/j-pro/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/assets/default/assets/pages/j-pro/css/j-pro-modern.css">
    <link href="~/assets/sweet_alert_2/sweetalert2.css" rel="stylesheet" />
}
@section JsForHead {
    <script type="text/javascript" language="javascript" src="https://code.jquery.com/jquery-3.5.1.js"></script>
}
<div class="preloader3 loader-block pos-absolute" style="
                    z-index: 2;
                    margin: auto;
                    width: 100%;
                    height: 100%;
                    background-color: rgba(0, 0, 0, 0.5); " id="spinner">
    <div class="circ1"></div>
    <div class="circ2"></div>
    <div class="circ3"></div>
    <div class="circ4"></div>
</div>
<div class="main-body">
    <div class="page-wrapper">
        <div class="page-header">
            <div class="page-header-title">
                <h4>DASHBOARD</h4>
            </div>
            <div class="page-header-breadcrumb">
                <ul class="breadcrumb-title">
                    <li class="breadcrumb-item">
                        <a href="@Url.Action("index", "dashboard")">
                            <i class="icofont icofont-home"></i>
                        </a>
                    </li>
                    <li class="breadcrumb-item">
                        <a href="#!">dashboard</a>
                    </li>
                </ul>
            </div>
            <br /><br />
            <div class="row">
                <div class="col-md-12 col-xl-4">

                    <div class="card table-card widget-primary-card">
                        <div class="row-table">
                            <div class="jm-logo col-sm-3 card-block-big">
                                <a href="@Url.Action("index", "brandmap")" style="color: white;">
                                    <i class="ti-map-alt"></i>
                                </a>
                            </div>
                            <div class="col-sm-9">
                                <h4>MAP</h4>
                                <h6>map management</h6>
                            </div>
                        </div>
                    </div>
                    <div class="card table-card widget-success-card">
                        <div class="row-table">
                            <div class="jm-logo col-sm-3 card-block-big">
                                <a href="@Url.Action("index", "managementaccount")" style="color: white;">
                                    <i class="ti-user m-r-5"></i>
                                </a>
                            </div>
                            <div class="col-sm-9">
                                <h4>Account</h4>
                                <h6>account management</h6>
                            </div>
                        </div>
                    </div>
                    <div class="card-footer bg-inverse" style="height: 126px;">
                        <div class="row text-center" style="margin-top: 18px;">
                            <div class="col-xs-4 col-sm-4 col-lg-4">
                                <h4 id="motorcycle">0</h4>
                                <span>Motorcycle <i class="icofont icofont-motor-bike-alt text-success"></i></span>
                            </div>
                            <div class="col-xs-4 col-sm-4 col-lg-4">
                                <h4 id="truck">0</h4>
                                <span>Truck <i class="ti-truck text-warning"></i></span>
                            </div>
                            <div class="col-xs-4 col-sm-4 col-lg-4">
                                <h4 id="other">0</h4>
                                <span>Other <i class="ti-package text-success"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-xl-8">
                    <div class="card">
                        <div class="card-block" style="padding: 0px;">
                            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                                <ol class="carousel-indicators">
                                    <li data-target="#carouselExampleIndicators" data-slide-to="0" class=""></li>
                                    <li data-target="#carouselExampleIndicators" data-slide-to="1" class=""></li>
                                    <li data-target="#carouselExampleIndicators" data-slide-to="2" class="active"></li>
                                    <li data-target="#carouselExampleIndicators" data-slide-to="3" class=""></li>
                                </ol>
                                <div class="carousel-inner" role="listbox">
                                    <div class="carousel-item">
                                        <div class="grid" style="margin: 0px; width: 100%; max-width: 100%; padding: 0px; margin: 0px;">
                                            <figure class="effect-duke" style="width: 100% !important; max-width: 100%;">
                                                <img class="d-block img-fluid w-100" src="~/assets/default/assets/images/icon-trade-zone//2988.jpg"
                                                     alt="img27" />
                                                <figcaption>
                                                    <h2>Manage <span>Trade ZOne</span></h2>
                                                    <p>
                                                        Aid for dividing trade zone areas.
                                                    </p>
                                                    <a href="#">View more</a>
                                                </figcaption>
                                            </figure>

                                        </div>
                                    </div>
                                    <div class="carousel-item">
                                        <div class="grid" style="margin: 0px; width: 100%; max-width: 100%; padding: 0px; margin: 0px;">
                                            <figure class="effect-duke" style="width: 100% !important; max-width: 100%;">
                                                <img class="d-block img-fluid w-100" src="https://thuthuatnhanh.com/wp-content/uploads/2020/01/cac-mau-background-dep.jpg"
                                                     alt="img27" />
                                                <figcaption>
                                                    <h2>Messy <span>Duke</span></h2>
                                                    <p>
                                                        Duke is very bored. When he looks at the
                                                        sky, he feels to run.
                                                    </p>
                                                    <a href="#">View more</a>
                                                </figcaption>
                                            </figure>

                                        </div>
                                    </div>
                                    <div class="carousel-item active">
                                        <div class="grid" style="margin: 0px; width: 100%; max-width: 100%; padding: 0px; margin: 0px;">
                                            <figure class="effect-duke" style="width: 100% !important; max-width: 100%;">
                                                <img class="d-block img-fluid w-100" src="https://thuthuatnhanh.com/wp-content/uploads/2020/01/cac-mau-background-dep.jpg"
                                                     alt="img27" />
                                                <figcaption>
                                                    <h2>Messy <span>Duke</span></h2>
                                                    <p>
                                                        Duke is very bored. When he looks at the
                                                        sky, he feels to run.
                                                    </p>
                                                    <a href="#">View more</a>
                                                </figcaption>
                                            </figure>

                                        </div>
                                    </div>
                                    <div class="carousel-item">
                                        <div class="grid" style="margin: 0px; width: 100%; max-width: 100%; padding: 0px; margin: 0px;">
                                            <figure class="effect-duke" style="width: 100% !important; max-width: 100%;">
                                                <img class="d-block img-fluid w-100" src="https://thuthuatnhanh.com/wp-content/uploads/2020/01/cac-mau-background-dep.jpg"
                                                     alt="img27" />
                                                <figcaption>
                                                    <h2>Messy <span>Duke</span></h2>
                                                    <p>
                                                        Duke is very bored. When he looks at the
                                                        sky, he feels to run.
                                                    </p>
                                                    <a href="#">View more</a>
                                                </figcaption>
                                            </figure>

                                        </div>
                                    </div>
                                </div>
                                <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                </a>
                                <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                </a>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="card table-1-card">
                        <div class="card-header">
                            <h5>New building & store</h5>
                        </div>
                        <div class="card-block">
                            <div class="table-responsive">
                                <table class="table" id="history">
                                    <thead>
                                        <tr>
                                            <th class="text-center">No</th>
                                            <th class="text-center">Name</th>
                                            <th class="text-center">CreateDate</th>
                                            <th class="text-center">Type</th>
                                            <th class="text-center">Details</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tblData">
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section ScriptsForBottom {
    <script src="~/assets/default/assets/js/brand/brand-index.js"></script>
    <script src="~/assets/sweet_alert_2/sweetalert2.js"></script>
    <script type="text/javascript" src="~/assets/default/assets/pages/j-pro/js/jquery.ui.min.js"></script>
    <script type="text/javascript" src="~/assets/default/assets/pages/j-pro/js/jquery.maskedinput.min.js"></script>
    <script type="text/javascript" src="~/assets/default/assets/pages/j-pro/js/jquery.j-pro.js"></script>
}

