    <script type="text/javascript"
            src="~/assets/default/assets/pages/dashboard/custom-dashboard.js"></script>
    <script type="text/javascript" src="~/assets/default/assets/js/script.js"></script>

    <script src="~/assets/default/assets/js/pcoded.min.js"></script>
    <script src="~/assets/default/assets/js/demo-12.js"></script>
    <script src="~/assets/default/assets/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/assets/default/assets/js/jquery.mousewheel.min.js"></script>
    @RenderSection("ScriptsForBottom", required: false)
</body>
</html>