                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Distance</label>
                    <div class="col-sm-10">
                        <label id="distanceService-error" class="error" style="display: none; color: rgb(138, 31, 17);">
                            This is required an number greater than 0.
                        </label>
                        <input type="number" min="1" max="50" class="form-control" placeholder="km" id="distanceService">
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Stores</label>
                    <div class="col-sm-10">
                        <label id="storeSelect-error" class="error" style="display: none; color: rgb(138, 31, 17);">
                            Please choose one or more stores
                        </label>
                        <select id='storeSelect' multiple>
                            @if (Model != null) foreach (var item in Model)
                                {
                                    if (item.Status == 1 || item.Status == 2 || item.Status == 4)
                                    {
                                        <option value='@item.Id'>@item.Name</option>
                                    }
                                }
                        </select>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Day</label>
                    <div class="col-sm-10">
                        <label id="pickDay-error" class="error" style="display: none; color: rgb(138, 31, 17);">
                            Please choose one or more day
                        </label>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickDay('mondayLabel')" id="mondayLabel" style="font-weight: 700; font-size: 14px;">Mon</button>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickDay('tuesdayLabel')" id="tuesdayLabel" style="font-weight: 700; font-size: 14px;">Tue</button>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickDay('wednesdayLabel')" id="wednesdayLabel" style="font-weight: 700; font-size: 14px;">Wed</button>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickDay('thursdayLabel')" id="thursdayLabel" style="font-weight: 700; font-size: 14px;">Thu</button>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickDay('fridayLabel')" id="fridayLabel" style="font-weight: 700; font-size: 14px;">Fri</button>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickDay('saturdayLabel')" id="saturdayLabel" style="font-weight: 700; font-size: 14px;">Sat</button>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickDay('sundayLabel')" id="sundayLabel" style="font-weight: 700; font-size: 14px;">Sun</button>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Time</label>
                    <div class="col-sm-10">
                        <label id="pickTimeSlot-error" class="error" style="display: none; color: rgb(138, 31, 17);">
                            Please choose one or more day
                        </label>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickTimeSlot('labelTimeSlot1')" id="labelTimeSlot1" style="font-weight: 700; font-size: 14px;">0-6h</button>
