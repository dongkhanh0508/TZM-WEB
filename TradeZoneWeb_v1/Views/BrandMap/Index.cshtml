                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickTimeSlot('labelTimeSlot3')" id="labelTimeSlot3" style="font-weight: 700; font-size: 14px;">12-18h</button>
                        <button class="btn btn-mini btn-round btn-default m-b-5" onclick="PickTimeSlot('labelTimeSlot4')" id="labelTimeSlot4" style="font-weight: 700; font-size: 14px;">18-24h</button>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button"
                        id="closeTradeZoneModalButton"
                        class="btn btn-secondary"
                        data-dismiss="modal">
                    Close
                </button>
                <button type="button" class="btn btn-primary" onclick="InitTradeZone()">
                    Submit
                </button>
            </div>
        </div>
    </div>
</div>
