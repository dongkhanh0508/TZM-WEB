}
@section ScriptsForBottom {
    @*<script type="module" src="~/assets/default/assets/js/brand-map/brand-map-module.js"></script>*@
    <script type="text/javascript" src="~/assets/default/assets/js/mappick.js"></script>
    <script type="text/javascript" src="~/assets/default/assets/js/brand-map/map.js"></script>
    <script type="text/javascript" src="~/assets/multi_select/jquery.multi-select.js"></script>
    <script type="text/javascript" src="~/assets/default/assets/pages/j-pro/js/jquery.ui.min.js"></script>
    <script type="text/javascript" src="~/assets/default/assets/pages/j-pro/js/jquery.maskedinput.min.js"></script>
    <script type="text/javascript" src="~/assets/default/assets/pages/j-pro/js/jquery.j-pro.js"></script>
}
<div class="preloader3 loader-block pos-absolute"
     style="display: none;
                    z-index: 2;
                    margin: auto;
                    width: 100%;
                    height: 100%;
                    background-color: rgba(0, 0, 0, 0.5); "
     id="loadingMainScreen">
    <div class="circ1"></div>
    <div class="circ2"></div>
    <div class="circ3"></div>
    <div class="circ4"></div>
</div>
@* See detail store button *@
<button data-toggle="modal" data-target="#seeStoreDetail" id="seeStoreDetailButton" style="display:none">
</button>
@* See detail store button *@
<button data-toggle="modal" data-target="#seeBuildingDetail" id="seeBuildingDetailButton" style="display:none">
</button>
<div id="map" style="z-index: 0"></div>
<!-- Store list -->
<div class="card"
     id="listStores"
     style="position: absolute;
            padding-top:56px;
            top: 0px;
            left: 0%;
            height: 100%;
            width: 30%;
            transition: 0.5s;
            border-top: none !important;">
    <div class="card-header">
        <h5 class="m-t-10">Brand map dashboard</h5>
        <button class="btn btn-default btn-mini"
                style="height: 40.4px;
                       width: 44px;
                       top:66px;
                       position: absolute;
                       right: -44px;
                       background-color: white;"
                onclick="ShowStores()">
            <div style="color: grey;
                        font-size: 15px">
                <i class="zmdi zmdi-store"></i>
                <i class="zmdi zmdi-chevron-left" id="iconListStores"></i>
            </div>

        </button>
    </div>
    @*Tabs od store control*@
    <div class="card-block">
        <ul class="nav nav-tabs md-tabs" role="tablist">
            <li class="nav-item" id="listStoreNavItem">
                <a class="nav-link active" data-toggle="tab" href="#listStore" role="tab">Store</a>
                <div class="slide"></div>
            </li>
            <li class="nav-item" id="listTradeZoneNavItem">
                <a class="nav-link" data-toggle="tab" href="#tradeZone" role="tab">Trade zone</a>
                <div class="slide"></div>
            </li>
            <li class="nav-item" id="listGroupZoneNavItem">
                <a class="nav-link" data-toggle="tab" href="#groupZone" role="tab">Group zone</a>
                <div class="slide"></div>
            </li>
            <li class="nav-item" id="listAssetNavItem">
                <a class="nav-link" data-toggle="tab" href="#asset" role="tab">Asset</a>
                <div class="slide"></div>
            </li>
        </ul>

        <div class="tab-content card-block p-0 p-t-10">
            @*Tab store control tab*@
            <div class="tab-pane active" id="listStore" role="tabpanel">
                @*Filter by group zone*@
                <div class="form-group row">
                    <label class="col-sm-3 col-form-label">Group Zone</label>
                    <div class="col-sm-9">
                        <select name="select" class="form-control form-control-primary m-b-10"
                                id="filterStoreControlTab" onchange="FilterStoreByGroupZone()">
                            <option value="-1">All stores</option>
                        </select>
                    </div>
                </div>
                @*Checkbox show stores*@
                <div class="form-group row">
                    <label class="col-sm-3 col-form-label">Show</label>
                    <div class="col-sm-9">
                        <div class="checkbox-fade fade-in-primary" id="checkboxStoreTab">
                            <label>
                                <input type="checkbox" value="" id="showAllStoreCheckBox" onchange="StoreCheckBoxChange()">
                                <span class="cr">
                                    <i class="cr-icon icofont icofont-ui-check txt-primary"></i>
                                </span> <span>Store</span>
                            </label>
                        </div>
                        <div class="checkbox-fade fade-in-primary" >
                            <label>
                                <input type="checkbox" value="" id="showGroupZoneCheckBox" onchange="GroupZoneBoxChange()">
                                <span class="cr">
                                    <i class="cr-icon icofont icofont-ui-check txt-primary"></i>
                                </span> <span>Group</span>
                            </label>
                        </div>
                        <div class="checkbox-fade fade-in-primary" >
                            <label>
                                <input type="checkbox" value="" id="showTradeZoneCheckBox" onchange="TradeZoneBoxChange()">
                                <span class="cr">
                                    <i class="cr-icon icofont icofont-ui-check txt-primary"></i>
                                </span> <span>Trade zone</span>
                            </label>
                        </div>
                    </div>

                </div>
                @*Title for "No trade zone version active"*@
                <div style="text-align: center" id="noActiveTradeZoneNotification">
                    <i class="ti-na" style="color: red; font-size: 50px;"></i>
                    <h3>No trade zone version is active</h3>
                    <h6><i>Active a trade zone version first</i></h6>
                </div>
                @*Table for store of group zone*@
                <div class="table-responsive"
                     id="tableOfStoreOfGroupZone"
                     style="height: 65%;overflow: auto; display: none">
                    <table class="table table-striped table-bordered"
                           id="new-cons"
                           style="font-size: 13px">
                        <thead>
                            <tr>
                                <th class="text-center">#</th>
                                <th class="text-center">Name</th>
                                <th class="text-center">Weight</th>
                                <th class="tabledit-toolbar-column text-center">Action</th>
                            </tr>
                        </thead>
                        <tbody id="tbodyOfStoreOfGroupZone">
                        </tbody>
                    </table>
                </div>
            </div>
            @*Tab trade zone of store control tabs*@
            <div class="tab-pane" id="tradeZone" role="tabpanel">
                @*Filter by group zone*@
                <div class="form-group row" id="tradeZoneVersionRow">
                    <label class="col-sm-3 col-form-label">Version</label>
                    <div class="col-sm-7">
                        <select name="select" class="form-control form-control-primary" id="filterVersionTradeZone">
                            <option value="-1">Loading...</option>
                        </select>
                    </div>
                    <div class="col-sm-2">
                        @*Button init*@
                        <button class="btn btn-primary"
                                onclick=""
                                title="Init new trade zone"
                                id="btnInitNewTradeZone"
                                style="float: right"
                                data-toggle="modal"
                                data-target="#inputDistanceModal">
                            <i class="icofont icofont-plus" style="margin-right: 0px !important"></i>
                        </button>
                    </div>
                </div>
                @*Tab of group zone's store*@
                <div class="trade-zone-tab-content" id="storeOfGroupZoneTab">
                    <div style="float:left">
                        <h6 id="titleOfStoreTradeZone">Store of Group zone</h6>
                        <h6 id="coverageOfGroupZone">Coverage: 95%</h6>
                    </div>

                    <button class="btn btn-primary btn-mini m-l-5"
                            style="float: right"
                            id="btnCloseStoreOfGroupZone"
                            onclick="CloseStoreOfGroupZone()">
                        Back
                    </button>
                    @*Table of group zones*@
                    <div class="table-responsive p-t-10" style="height: 65%;overflow: auto;">
                        <table class="table table-striped table-bordered"
                               id="new-cons"
                               style="font-size: 13px">
                            <thead>
                                <tr>
                                    <th class="text-center">#</th>
                                    <th class="text-center">Store</th>
                                    <th class="text-center">Weight</th>
                                    <th class="tabledit-toolbar-column text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody id="bodyOfStoreTradeZoneOfGroupZone">
                            </tbody>
                        </table>
                    </div>
                </div>
                @*Tab of group zone of trade zone*@
                <div class="trade-zone-tab-content" id="groupZoneOfTradeZoneTab">
                    <div style="float:left">
                        <h6 id="titleOfGroupZone">Group zone of Trade zone</h6>
                    </div>
                    <button class="btn btn-primary btn-mini m-l-5"
                            style="float: right"
                            id="btnCloseGroupZoneOfTradeZone"
                            onclick="CloseGroupZoneOfTradeZone()">
                        Back
                    </button>
                    @*Table of group zones*@
                    <div class="table-responsive p-t-10" style="height: 65%;overflow: auto;">
                        <table class="table table-striped table-bordered"
                               id="new-cons"
                               style="font-size: 13px">
                            <thead>
                                <tr>
                                    <th class="text-center">#</th>
                                    <th class="text-center">Group zone</th>
                                    <th class="text-center">Coverage</th>
                                    <th class="tabledit-toolbar-column text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody id="bodyGroupZoneOfTradeZoneVersionTable">
                            </tbody>
                        </table>
                    </div>
                </div>
                @*Tab of detail trade zone*@
                @*Tab of initing*@
                <div class="trade-zone-tab-content" id="initingTradeZoneTab">
                    <h5>Initial version</h5>
                    @*Group button confirm*@
                    <div style="float: right" id="confirmGroupButton" class="m-b-10">
                        <button class="btn btn-danger btn-mini m-l-5"
                                style="float: right"
                                id="btnCloseTradeZone"
                                onclick="CloseConfirmBox()">
                            Cancel
                        </button>
                        <button class="btn btn-success btn-mini"
                                style="float: right"
                                id="btnConfirmTradeZone"
                                data-toggle="modal"
                                data-target="#inputInfoToInsertTradeZone">
                            Confirm
                        </button>
                    </div>
                    @*Table of initing*@
                    <div class="table-responsive" style="height: 65%;overflow: auto;">
                        <table class="table table-striped table-bordered"
                               id="new-cons"
                               style="font-size: 13px">
                            <thead>
                                <tr>
                                    <th class="text-center">#</th>
                                    <th class="text-center">Trade Zone of store</th>
                                    <th class="tabledit-toolbar-column text-center">Weight</th>
                                </tr>
                            </thead>
                            <tbody id="bodyInitingTradeZone">
                            </tbody>
                        </table>
                    </div>
                </div>
                @**@
                <div>
                    @*Button back*@
                    <div class="checkbox-fade fade-in-primary">
                        <label>
                            <input type="checkbox" value="" id="detailTradeZoneStatus" onclick="return false;">
                            <span class="cr">
                                <i class="cr-icon icofont icofont-ui-check txt-primary"></i>
                            </span> <span>Active</span>
                        </label>
                    </div>
                    <button class="btn btn-danger btn-mini m-l-5"
                            style="float: right"
                            onclick="DeleteTradeZoneVersionDetail()">
                        Delete
                    </button>
                    <button class="btn btn-success btn-mini m-l-5"
                            id="activeTradeZoneButton"
                            style="float: right"
                            onclick="ActiveTradeZoneVersion()">
                        Active
                    </button>
                    @*Div detail*@
                    <div style="height: 55%; overflow: auto; overflow-x: hidden;">
                        @*id*@
                        <input type="hidden" name="name" id="detailTradeZoneId" value="" />
                        @*Day*@
                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Day</label>
                            <div class="col-sm-10">
                                <label class="badge badge-lg" id="monDetailLabel">Mon</label>
                                <label class="badge badge-lg" id="tueDetailLabel">Tue</label>
                                <label class="badge badge-lg" id="wedDetailLabel">Wed</label>
                                <label class="badge badge-lg" id="thuDetailLabel">Thu</label>
                                <label class="badge badge-lg" id="friDetailLabel">Fri</label>
                                <label class="badge badge-lg" id="satDetailLabel">Sat</label>
                                <label class="badge badge-lg" id="sunDetailLabel">Sun</label>
                            </div>
                        </div>
                        @*Slot*@
                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Slot</label>
                            <div class="col-sm-10">
                                <label class="badge badge-lg" id="slot1DetailLabel">0-6h</label>
                                <label class="badge badge-lg" id="slot2DetailLabel">6-12h</label>
                                <label class="badge badge-lg" id="slot3DetailLabel">12-18h</label>
                                <label class="badge badge-lg" id="slot4DetailLabel">18-24h</label>
                            </div>
                        </div>
                        @*Distance*@
                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Distance</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="detailTradeZoneDistance" readonly>
                            </div>
                        </div>
                        @*List tradezone of store*@
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered"
                                   id="new-cons"
                                   style="font-size: 13px">
                                <thead>
                                    <tr>
                                        <th class="text-center">#</th>
                                        <th class="text-center">Trade Zone of store</th>
                                        <th class="tabledit-toolbar-column text-center">Weight</th>
                                        <th class="text-center">Action</th>
                                    </tr>
                                </thead>
                                <tbody id="bodyTradeZoneOfStoreTable">
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            @*Tab group zone control tab*@
            <div class="tab-pane" id="groupZone" role="tabpanel">
                <button class="btn btn-primary btn-mini"
                        id="InitSystemZoneForGroupZoneButton"
                        style="float: right"
                        data-toggle="modal"
                        data-target="#chooseTypeGroupZoneModal">
                    Create new
                </button>

                <div id="groupGroupZoneButton" style="display: none">
                    <button class="btn btn-danger btn-mini" onclick="CancelSystemZoneForGroupZone()"
                            id="CancelSystemZoneForGroupZoneButton"
                            style="float: right;">
                        <i class="icofont icofont-ui-close" style="margin-right:0px"></i>
                    </button>
                    <button class="btn btn-success btn-mini m-r-5"
                            onclick="CheckValidGroupZone()"
                            style="float: right;">
                        <i class="icofont icofont-ui-check" style="margin-right:0px"></i>
                    </button>
                </div>

                <div class="checkbox-fade fade-in-primary">
                    <label>
                        <input type="checkbox" value="" id="showAllGroupZoneCheckBox">
                        <span class="cr">
                            <i class="cr-icon icofont icofont-ui-check txt-primary"></i>
                        </span> <span>Show group Zone on map</span>
                    </label>
                </div>
                <div class="table-responsive" style="height: 65%; overflow: auto;">
                    <table class="table table-striped table-bordered"
                           id="new-cons"
                           style="font-size: 13px">
                        <thead>
                            <tr>
                                <th class="text-center">Id</th>
                                <th class="text-center">Group Zone</th>
                                <th class="tabledit-toolbar-column text-center">Action</th>
                            </tr>
                        </thead>
                        <tbody id="bodyGroupZoneTable">
                            <tr>
                                <th class="text-center" scope="row">Loading...</th>
                                <td class="tabledit-view-mode">
                                    <span class="tabledit-span">Loading...</span>
                                </td>
                                <td style="white-space: nowrap; width: 1%">
                                    <span class="tabledit-span">Loading...</span>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            @*Tab asset control tab*@
            <div class="tab-pane" id="asset" role="tabpanel">
                <div style="float:left">
                    <h6 id="">Tracking assets</h6>
                </div>
                <button class="btn btn-success btn-mini m-l-5 m-b-10"
                        style="float: right"
                        id="trackingButton"
                        onclick="TrackingAsset()">
                    Tracking
                </button>
                <button class="btn btn-danger btn-mini m-l-5 m-b-10"
                        style="float: right; display: none;"
                        id="stopTrackingButton"
                        onclick="StopTrackingAsset()">
                    Stop
                </button>
                <div class="table-responsive" style="height: 65%; overflow: auto; display: none" id="tableOfTrackingAsset">
                    <table class="table table-striped table-bordered"
                           id="new-cons"
                           style="font-size: 13px">
                        <thead>
                            <tr>
                                <th class="text-center">#</th>
                                <th class="text-center">Name</th>
                                <th class="tabledit-toolbar-column text-center">Type</th>
                                <th class="tabledit-toolbar-column text-center">Action</th>
                            </tr>
                        </thead>
                        <tbody id="bodyOfTrackingAsset">
                            <tr>
                                <th class="text-center" scope="row">Loading...</th>
                                <td class="tabledit-view-mode">
                                    <span class="tabledit-span">Loading...</span>
                                </td>
                                <td style="white-space: nowrap; width: 1%">
                                    <span class="tabledit-span">Loading...</span>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
</div>
<!-- Modal detail building -->
<div class="modal fade"
     id="seeBuildingDetail"
     style="z-index: 1060">
    <div class="modal-dialog modal-lg" role="document" id="buildingModal">
        <div class="preloader3 loader-block pos-absolute"
             style="display: none;
                    z-index: 2;
                    margin: auto;
                    width: 100%;
                    height: 100%;
                    background-color:rgba(0, 0, 0, 0.5);"
             id="buildingPreloader">
            <div class="circ1"></div>
            <div class="circ2"></div>
            <div class="circ3"></div>
            <div class="circ4"></div>
        </div>
        <div class="modal-content">
            <div class="modal-header" style="display: block;">
                <img id="buildingImg" src="http://maps.gstatic.com/tactile/pane/default_geocode-2x.png"
                     width="100%"
                     height="210px"
                     style="object-fit: cover" />
                <div class="row m-t-10">
                    <div class="col-sm-8">
                        <h3 class="modal-title" id="buildingName">
                        </h3>
                    </div>
                </div>
                <div class="row">
                    <h6 id="buildingType" class="col-sm-6"></h6>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div id="infoBuildingRow" class="col-sm-6">
                        <div class="form-group row">
                            <label class="col-sm-4 col-form-label">Id</label>
                            <div class="col-sm-8">
                                <input type="text"
                                       class="form-control"
                                       id="buildingId"
                                       readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-sm-4 col-form-label">Address</label>
                            <div class="col-sm-8">
                                <input type="text"
                                       class="form-control"
                                       id="buildingAdress"
                                       readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-sm-4 col-form-label">Create day</label>
                            <div class="col-sm-8">
                                <input type="text"
                                       class="form-control"
                                       id="buildingCreateDay"
                                       readonly />
                            </div>
                        </div>
